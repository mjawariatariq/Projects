create database IF NOT EXISTS employeemanagement;
use employeemanagement;
create table if NOT EXISTS login (username varchar(20), password varchar(20));
insert into login values('jawaria', '12345');

create table if NOT EXISTS employee (
    name varchar(40), 
    fname varchar(40), 
    dob varchar(40), 
    salary varchar(30), 
    address varchar(20), 
    phone varchar(40), 
    email varchar(40), 
    education varchar(40), 
    designation varchar(40)
);
insert into employee values('Jawaria', 'Tariq', '2006-03-27', '60000', 'Nawabshah', '123456789', 'mjawariatariq@gmail.com', 'BSC', 'AI Engineer');

select * from employee;
