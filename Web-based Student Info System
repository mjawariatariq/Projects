import java.awt.Desktop;
import java.io.IOException;
import java.net.URI;
import java.net.URISyntaxException;
import java.util.ArrayList;
import java.util.Scanner;

class Student {
    String name;
    String department;
    int rollNumber;
    String phoneNumber;
    String cnic;
    String gender;
    String address;

    // Constructor
    public Student(String name, String department, int rollNumber, String phoneNumber, String cnic, String gender, String address) {
        this.name = name;
        this.department = department;
        this.rollNumber = rollNumber;
        this.phoneNumber = phoneNumber;
        this.cnic = cnic;
        this.gender = gender;
        this.address = address;
    }
}

public class StudentInformationSystem {
    private ArrayList<Student> students;

    // Constructor
    public StudentInformationSystem() {
        students = new ArrayList<>();
    }

    // Method to add a student
    public void addStudent(Student student) {
        students.add(student);
    }

    // Method to search for a student by name, CNIC, series number, or phone number
    public Student searchStudent(String searchTerm) {
        for (Student student : students) {
            if (student.name.equalsIgnoreCase(searchTerm) ||
                student.cnic.equals(searchTerm) ||
                String.valueOf(student.rollNumber).contains(searchTerm) ||
                student.phoneNumber.equals(searchTerm)) {
                return student;
            }
        }
        return null; // Return null if no student found
    }

    public static void main(String[] args) {
        // Creating a new StudentInformationSystem object
        StudentInformationSystem sis = new StudentInformationSystem();

        // Adding some sample students
        sis.addStudent(new Student("John Doe", "Computer Science", 101, "1234567890", "12345-6789012-3", "Male", "123 Main St"));
        sis.addStudent(new Student("Jane Smith", "Electrical Engineering", 102, "9876543210", "98765-4321098-7", "Female", "456 Elm St"));

        // Taking user input for search term
        Scanner scanner = new Scanner(System.in);
        System.out.println("Enter search term (name, CNIC, roll number, or phone number):");
        String searchTerm = scanner.nextLine();

        // Searching for the student
        Student foundStudent = sis.searchStudent(searchTerm);

        // Displaying the student information if found
        if (foundStudent != null) {
            System.out.println("Student Found:");
            System.out.println("Name: " + foundStudent.name);
            System.out.println("Department: " + foundStudent.department);
            System.out.println("Roll Number: " + foundStudent.rollNumber);
            System.out.println("Phone Number: " + foundStudent.phoneNumber);
            System.out.println("CNIC: " + foundStudent.cnic);
            System.out.println("Gender: " + foundStudent.gender);
            System.out.println("Address: " + foundStudent.address);

            // Open a web page
            openWebPage("https://example.com"); // Replace "https://example.com" with the URL of the web page you want to open
        } else {
            System.out.println("Student not found.");
        }
    }

    // Method to open a web page
    public static void openWebPage(String url) {
        try {
            Desktop.getDesktop().browse(new URI(url));
        } catch (IOException | URISyntaxException e) {
            System.out.println("Error opening the web page: " + e.getMessage());
        }
    }
}